target=sampling.evaluation.lawdb.AdaptiveAttack

classpath=${jpf-security}/build/main;${jpf-security}/build/examples
sourcepath=${jpf-security}/src/examples

#listener=sidechannel.singlerun.PathInspector

mutirun.secure_method = sampling.evaluation.lawdb.AdaptiveAttack.cost

sidechannel = timing

greedy=false

adaptive=maxcc

multirun.num_run = 5
sidechannel.high_input_size = 1
sidechannel.low_input_size = 2

sidechannel.min_high=1
sidechannel.max_high=100

symbolic.min_int=1
symbolic.max_int=100

cost.interval = 2

SMT.theory = linear

sidechannel.smt2 = ${jpf-security}/build/tmp/lawdb.smt2

symbolic.dp = z3

sidechannel.verbose = false

sidechannel.debug = false