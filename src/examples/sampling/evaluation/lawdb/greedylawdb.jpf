target=sampling.evaluation.lawdb.GreedyAdaptive

classpath=${jpf-security}/build/main;${jpf-security}/build/examples
sourcepath=${jpf-security}/src/examples

sidechannel = timing

mutirun.secure_method = sampling.evaluation.lawdb.GreedyAdaptive.foo

adaptive=tree

attack.tree=entropy

multirun.num_run = 2
sidechannel.high_input_size = 1
